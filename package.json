{
  "name": "cloud-cinema-server",
  "version": "1.0.0",
  "engines": {
    "node": "12.13.0",
    "npm": "6.12.0"
  },
  "description": "Cloud Cinema Server Project",
  "private": true,
  "scripts": {
    "client": "cd client && npm run start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ochanissi/Cloud-Cinema.git"
  },
